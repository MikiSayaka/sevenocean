      <div id="page-wrapper">
        <div class="row">
          <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <h1 class="page-header">Empty Position</h1>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <form class="form-horizontal container-form" id="emptyPositionForm" name="emptyPositionForm" action="emptyPosition" method="post">
              <!--{{{-->
              <div class="row">
                <div class="form-group col-lg-6">
                  <label for="container_no" class="col-lg-4 control-label">CONTAINER NO.</label>
                  <div class="col-lg-4">
                    <input type="text" id="container_no" name="container_no" class="form-control" placeholder="CONTAINER NO.">
                  </div>
                </div>
                <div class="form-group col-lg-6 col-lg-pull-2">
                  <label for="type" class="col-lg-4 control-label">TYPE</label>
                  <div class="col-lg-4">
                    <input type="text" id="type" name="type" class="form-control" placeholder="TYPE">
                  </div>
                </div>
              </div>
              <!--}}}-->
              <!--{{{-->
              <div class="row">
                <div class="form-group col-lg-6">
                  <label for="container_no" class="col-lg-4 control-label">FROM</label>
                  <div class="col-lg-4">
                    <input type="text" id="container_from" name="container_from" class="form-control" placeholder="FROM">
                  </div>
                </div>
                <div class="form-group col-lg-6 col-lg-pull-2">
                  <label for="type" class="col-lg-4 control-label">TO</label>
                  <div class="col-lg-4">
                    <input type="text" id="container_to" name="container_to" class="form-control" placeholder="TO">
                  </div>
                </div>
              </div>
              <!--}}}-->
              <!--{{{-->
              <div class="row">
                <div class="form-group col-lg-6">
                  <label class="col-lg-4 control-label">&nbsp;</label>
                  <div class="col-lg-4">
                    <button class="btn btn-primary" id="submit">送出</button>
                    <button class="btn btn-danger" id="reset">清空</button>
                  </div>
                  <div class="col-lg-4">
                    <div class="form-check">
                      <input class="form-check-input" type="checkbox" id="keepValue" value="keepValue">
                      <label class="form-check-label" for="keepValue">Keep Value</label>
                    </div>
                  </div>
                </div>
              </div>
              <!--}}}-->
            </form>
          </div>
        </div>
        <hr>
        <div class="row">
          <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 submit-form">
            <form class="form-inline"  id="emptyPositionFormQueryByDate" name="emptyPositionFormQueryByDate" action="emptyPosition" method="post">
              <div class="form-group">
                <div class="input-group input-daterange">
                  <input type="text" class="form-control" id="start_date" name="start_date" placeholder="START DATE" data-date-format="yyyy/mm/dd">
                  <div class="input-group-addon">TO</div>
                  <input type="text" class="form-control" id="end_date" name="end_date" placeholder="END DATE" data-date-format="yyyy/mm/dd">
                </div>
              </div>
              <button type="submit" class="btn btn-primary" id="query_by_date">查詢</button>
            </form>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 data-table">
            <div>
              <table class="table table-bordered">
                <thead>
                  <tr>
                    <th>CONTAINER NO.</th>
                    <th>TYPE</th>
                    <th>FROM</th>
                    <th>TO</th>
                  </tr>
                </thead>
                <tbody>
                <% container_data.forEach(function(_containerItem) {%>
                  <tr>
                    <td><%= _containerItem.CONTAINER_NO %></td>
                    <td><%= _containerItem.CONTAINER_TYPE %></td>
                    <td><%= _containerItem.CONTAINER_FROM %></td>
                    <td><%= _containerItem.CONTAINER_TO %></td>
                  </tr>
                <% });%>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
